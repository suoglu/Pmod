# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  #Adding Group
  set Information [ipgui::add_group $IPINST -name "Information" -parent ${Page_0}]
  ipgui::add_static_text $IPINST -name "Description" -parent ${Information} -text {AXI4-Lite interface for Pmod DPOT, 6Ω-10kΩ digital potentiometer, AD5160}

  #Adding Group
  set Usage [ipgui::add_group $IPINST -name "Usage" -parent ${Page_0}]
  ipgui::add_static_text $IPINST -name "How to" -parent ${Usage} -text {Read and write to the base address to control potentiometer}



}

